<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid"
	xmlns:tns="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid"
	xmlns:SOAP-ENC="http://schemas.xmlsoap.org/soap/encoding/">
	
	<annotation>
	  	<documentation xml:lang="ee">
	  		ADIT(Avalike Dokumentide Infrastruktuuri Teenus) päringutes kasutatavate
	  		andmete kirjeldus.
	  		Dokument sisaldab nii (SOAP) päringute keha elementide kui ka päringute
	  		manustes kasutatavate andmete kirjeldusi.
	  		Litsenseeritud [LITSENTS] alusel.
	  	</documentation>
	  	<documentation xml:lang="en">
	  		Describes datatypes used in ADIT(Infrastructure Service for Public
	  		Documents) requests.
	  		The Documents containts both the datatypes used in (SOAP) request body
	  		elements and attachments.
	  		Licensed under [LICENSE].
	  	</documentation>
  	</annotation>
	
	<element name="joinRequest" type="tns:joinRequest"/>
	<element name="joinResponse" type="tns:joinResponse"/>
	<element name="unJoinRequest" type="tns:unJoinRequest"/>
	<element name="unJoinResponse" type="tns:unJoinResponse"/>
	<element name="getUserInfoRequest" type="tns:getUserInfoRequest"/>
	<element name="getUserInfoResponse" type="tns:getUserInfoResponse"/>
	<element name="getJoinedRequest" type="tns:getJoinedRequest"/>
	<element name="getJoinedResponse" type="tns:getJoinedResponse"/>
	<element name="getNotificationsRequest" type="tns:getNotificationsRequest"/>
	<element name="getNotificationsResponse" type="tns:getNotificationsResponse"/>
	<element name="setNotificationsRequest" type="tns:setNotificationsRequest"/>
	<element name="setNotificationsResponse" type="tns:setNotificationsResponse"/>
	<element name="getDocumentListRequest" type="tns:getDocumentListRequest"/>
	<element name="getDocumentListResponse" type="tns:getDocumentListResponse"/>
	<element name="getDocumentRequest" type="tns:getDocumentRequest"/>
	<element name="getDocumentResponse" type="tns:getDocumentResponse"/>
	<element name="getDocumentFileRequest" type="tns:getDocumentFileRequest"/>
	<element name="getDocumentFileResponse" type="tns:getDocumentFileResponse"/>
	<element name="saveDocumentRequest" type="tns:saveDocumentRequest"/>
	<element name="saveDocumentResponse" type="tns:saveDocumentResponse"/>
	<element name="saveDocumentFileRequest" type="tns:saveDocumentFileRequest"/>
	<element name="saveDocumentFileResponse" type="tns:saveDocumentFileResponse"/>
	<element name="deleteDocumentFileRequest" type="tns:deleteDocumentFileRequest"/>
	<element name="deleteDocumentFileResponse" type="tns:deleteDocumentFileResponse"/>
	<element name="deflateDocumentRequest" type="tns:deflateDocumentRequest"/>
	<element name="deflateDocumentResponse" type="tns:deflateDocumentResponse"/>
	<element name="deleteDocumentRequest" type="tns:deleteDocumentRequest"/>
	<element name="deleteDocumentResponse" type="tns:deleteDocumentResponse"/>
	<element name="prepareSignatureRequest" type="tns:prepareSignatureRequest"/>
	<element name="prepareSignatureResponse" type="tns:prepareSignatureResponse"/>
	<element name="confirmSignatureRequest" type="tns:confirmSignatureRequest"/>
	<element name="confirmSignatureResponse" type="tns:confirmSignatureResponse"/>
	<element name="shareDocumentRequest" type="tns:shareDocumentRequest"/>
	<element name="shareDocumentResponse" type="tns:shareDocumentResponse"/>
	<element name="unShareDocumentRequest" type="tns:unShareDocumentRequest"/>
	<element name="unShareDocumentResponse" type="tns:unShareDocumentResponse"/>
	<element name="markDocumentViewedRequest" type="tns:markDocumentViewedRequest"/>
	<element name="markDocumentViewedResponse" type="tns:markDocumentViewedResponse"/>
	<element name="modifyStatusRequest" type="tns:modifyStatusRequest"/>
	<element name="modifyStatusResponse" type="tns:modifyStatusResponse"/>
	<element name="getDocumentHistoryRequest" type="tns:getDocumentHistoryRequest"/>
	<element name="getDocumentHistoryResponse" type="tns:getDocumentHistoryResponse"/>
	<element name="sendDocumentRequest" type="tns:sendDocumentRequest"/>
	<element name="sendDocumentResponse" type="tns:sendDocumentResponse"/>
	
	<simpleType name="success">
		<annotation>
			<documentation xml:lang="ee">Näitab, kas päring õnnestus</documentation>
			<documentation xml:lang="en">Indicates if the request was successful</documentation>
		</annotation>
		<restriction base="boolean" />
	</simpleType>
	
	<simpleType name="href">
		<annotation>
			<documentation xml:lang="ee">Viide manusele</documentation>
			<documentation xml:lang="en">Attachment reference</documentation>
		</annotation>
		<restriction base="string" />
	</simpleType>
	
	<complexType name="message">
		<annotation>
	      	<documentation xml:lang="ee">Teade</documentation>
	      	<documentation xml:lang="en">Message</documentation>
    	</annotation>
		<simpleContent>
			<extension base="string">
				<attribute name="lang" type="string" />
			</extension>
		</simpleContent>
	</complexType>

	<complexType name="arrayOfMessage">
		<annotation>
	      	<documentation xml:lang="ee">Teadete massiiv</documentation>
	      	<documentation xml:lang="en">Array of messages</documentation>
    	</annotation>
		<sequence>
			<element name="message" type="tns:message" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfFileId">
		<annotation>
	      	<documentation xml:lang="ee">Failide identifikaatorite massiiv</documentation>
	      	<documentation xml:lang="en">Array of file identificators</documentation>
    	</annotation>
		<sequence>
			<element name="file_id" type="integer" maxOccurs="unbounded" minOccurs="0">
				<annotation>
			      	<documentation xml:lang="ee">Faili identifikaator</documentation>
			      	<documentation xml:lang="en">File identificator</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfDocumentType">
		<annotation>
	      	<documentation xml:lang="ee">Dokumendi tüüpide massiiv</documentation>
	      	<documentation xml:lang="en">Array of document types</documentation>
    	</annotation>
		<sequence>
			<element name="document_type" type="string" maxOccurs="unbounded" minOccurs="0">
				<annotation>
			      	<documentation xml:lang="ee">Dokumendi tüüp</documentation>
			      	<documentation xml:lang="en">Document type</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfDocumentDvkStatus">
		<annotation>
	      	<documentation xml:lang="ee">Dokumendi DVK(Dokumendivahetuskeskus) staatuste identifikaatorite massiiv</documentation>
	      	<documentation xml:lang="en">Array of document DVK(Dokumendivahetuskeskus) status identificators</documentation>
    	</annotation>
		<sequence>
			<element name="status_id" type="integer" maxOccurs="unbounded" minOccurs="0">
				<annotation>
			      	<documentation xml:lang="ee">Dokumendi DVK(Dokumendivahetuskeskus) staatuse identifikaator</documentation>
			      	<documentation xml:lang="en">Document DVK(Dokumendivahetuskeskus) status identificator</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfDocumentWorkflowStatus">
		<annotation>
	      	<documentation xml:lang="ee">Dokumendi töövoo staatuste identifikaatorite massiiv</documentation>
	      	<documentation xml:lang="en">Array of document workflow status identificators</documentation>
    	</annotation>
		<sequence>
			<element name="status_id" type="integer" maxOccurs="unbounded" minOccurs="0">
				<annotation>
			      	<documentation xml:lang="ee">Dokumendi töövoo staatuse identifikaator</documentation>
			      	<documentation xml:lang="en">Document workflow status identificator</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfCreatorApplication">
		<annotation>
	      	<documentation xml:lang="ee">Dokumendi loomiseks kasutatud infosüsteemide lühinimede massiiv</documentation>
	      	<documentation xml:lang="en">Array of the names of the applications used to create documents</documentation>
    	</annotation>
		<sequence>
			<element name="creator_application" type="string" maxOccurs="unbounded" minOccurs="0">
				<annotation>
			      	<documentation xml:lang="ee">Dokumendi loomiseks kasutatud infosüsteemi lühinimi</documentation>
			      	<documentation xml:lang="en">Name of the application used to create documents</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfUserCode">
		<annotation>
	      	<documentation xml:lang="ee">Kasutajakoodide massiiv</documentation>
	      	<documentation xml:lang="en">Array of user codes</documentation>
    	</annotation>
		<sequence>
			<element name="code" type="string" maxOccurs="unbounded" minOccurs="0">
				<annotation>
			      	<documentation xml:lang="ee">Kasutajakood</documentation>
			      	<documentation xml:lang="en">User codes</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="recipientStatus">
		<annotation>
	      	<documentation xml:lang="ee">Vastuvõtja(adressaat) staatus</documentation>
	      	<documentation xml:lang="en">Recipient status</documentation>
    	</annotation>
		<sequence>
			<element name="code" type="string">
				<annotation>
			      	<documentation xml:lang="ee">Vastuvõtja(adressaadi) kood</documentation>
			      	<documentation xml:lang="en">Recipient code</documentation>
		    	</annotation>
			</element>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage"/>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfRecipientStatus">
		<annotation>
	      	<documentation xml:lang="ee">Vastuvõtja(adressaadi) staatuste massiiv</documentation>
	      	<documentation xml:lang="en">Array of recipient statuses</documentation>
    	</annotation>
		<sequence>
			<element name="recipient" type="tns:recipientStatus" maxOccurs="unbounded" minOccurs="0"></element>
		</sequence>
	</complexType>
	
	<complexType name="notification">
		<annotation>
	      	<documentation xml:lang="ee">Teavitus</documentation>
	      	<documentation xml:lang="en">Notification</documentation>
    	</annotation>
		<sequence>
			<element name="type" type="string">
				<annotation>
			      	<documentation xml:lang="ee">Teavituse tüüp</documentation>
			      	<documentation xml:lang="en">Notification type</documentation>
		    	</annotation>
			</element>
			<element name="active" type="boolean">
				<annotation>
			      	<documentation xml:lang="ee">Näitab, kas teavituse tüüp on aktiivne</documentation>
			      	<documentation xml:lang="en">Indicates if the notification type is active</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfNotification">
		<annotation>
	      	<documentation xml:lang="ee">Teavituste massiiv</documentation>
	      	<documentation xml:lang="en">Array of notifications</documentation>
    	</annotation>
		<sequence>
			<element name="notification" type="tns:notification" maxOccurs="unbounded" minOccurs="0"></element>
		</sequence>
	</complexType>
	
	<complexType name="emailAddress">
		<annotation>
	      	<documentation xml:lang="ee">E-posti aadress</documentation>
	      	<documentation xml:lang="en">E-mail address</documentation>
    	</annotation>
		<sequence>
			<element name="address" type="string">
				<annotation>
			      	<documentation xml:lang="ee">E-posti aadress</documentation>
			      	<documentation xml:lang="en">E-mail address</documentation>
		    	</annotation>
			</element>
			<element name="redirected_to" type="string">
				<annotation>
			      	<documentation xml:lang="ee">Suunamise e-posti aadress</documentation>
			      	<documentation xml:lang="en">Redirection e-mail address</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="arrayOfEmailAddress">
		<annotation>
	      	<documentation xml:lang="ee">E-posti aadresside massiiv</documentation>
	      	<documentation xml:lang="en">Array of e-mail addresses</documentation>
    	</annotation>
		<sequence>
			<element name="email_address" type="tns:emailAddress" maxOccurs="unbounded" minOccurs="0"></element>
		</sequence>
	</complexType>

	<complexType name="joinRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "join"</documentation>
	      	<documentation xml:lang="en">"join" request</documentation>
    	</annotation>
		<sequence>
			<element name="userType" type="string">
				<annotation>
			      	<documentation xml:lang="ee">Kasutajatüüp</documentation>
			      	<documentation xml:lang="en">Usertype</documentation>
		    	</annotation>
			</element>
			<element name="userName" type="string">
				<annotation>
			      	<documentation xml:lang="ee">Kasutajanimi</documentation>
			      	<documentation xml:lang="en">Username</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="joinResponse">
		<annotation>
	      	<documentation xml:lang="ee">Päringu "join" vastus</documentation>
	      	<documentation xml:lang="en">"join" request response</documentation>
    	</annotation>
		<sequence>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage"></element>
		</sequence>
	</complexType>
	
	<!-- unJoin päringu tüübid -->
	<complexType name="unJoinRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "unJoin"</documentation>
	      	<documentation xml:lang="en">"unJoin" request</documentation>
    	</annotation>
		<sequence/>
	</complexType>
	<complexType name="unJoinResponse">
		<annotation>
	      	<documentation xml:lang="ee">Päringu "unJoin" vastus</documentation>
	      	<documentation xml:lang="en">"unJoin" request response</documentation>
    	</annotation>
		<sequence>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage"></element>
		</sequence>
	</complexType>
	
	<!-- getUserInfo päringu tüübid -->
	<complexType name="getUserInfoRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "getUserInfo"</documentation>
	      	<documentation xml:lang="en">"getUserInfo" request</documentation>
    	</annotation>
		<sequence>
			<element name="userList">
				<annotation>
			      	<documentation xml:lang="ee">Kasutajate massiiv</documentation>
			      	<documentation xml:lang="en">Array of users</documentation>
		    	</annotation>
				<complexType>
					<attribute name="href" type="tns:href" />
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="getUserInfoResponse">
		<annotation>
	      	<documentation xml:lang="ee">Päringu "getUserInfo" vastus</documentation>
	      	<documentation xml:lang="en">"getUserInfo" request response</documentation>
    	</annotation>
		<sequence>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage" />
			<element name="userList">
				<annotation>
			      	<documentation xml:lang="ee">Kasutajate andmete massiiv</documentation>
			      	<documentation xml:lang="en">Array of user data</documentation>
		    	</annotation>
				<complexType>
					<attribute name="href" type="tns:href" />
				</complexType>
			</element>
		</sequence>
	</complexType>
	
	<!-- getJoined päringu tüübid -->
	<complexType name="getJoinedRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "getJoined"</documentation>
	      	<documentation xml:lang="en">"getJoined" request</documentation>
    	</annotation>
		<sequence>
			<element name="maxResults" type="integer" minOccurs="0" maxOccurs="1">
				<annotation>
			      	<documentation xml:lang="ee">Makimaalne tagastatavate tulemuste arv</documentation>
			      	<documentation xml:lang="en">Maximum number of results returned</documentation>
		    	</annotation>
			</element>
			<element name="startIndex" type="integer" minOccurs="0" maxOccurs="1">
				<annotation>
			      	<documentation xml:lang="ee">Tagastatavate tulemuste algusindeks</documentation>
			      	<documentation xml:lang="en">Offset(starting index) of the results returned</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="getJoinedResponse">
		<annotation>
	      	<documentation xml:lang="ee">Päringu "getJoined" vastus</documentation>
	      	<documentation xml:lang="en">"getJoined" request response</documentation>
    	</annotation>
		<sequence>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage" />
			<element name="userList">
				<annotation>
			      	<documentation xml:lang="ee">Kasutajate andmete massiiv</documentation>
			      	<documentation xml:lang="en">Array of user data</documentation>
		    	</annotation>
				<complexType>
					<attribute name="href" type="tns:href" />
				</complexType>
			</element>
		</sequence>
	</complexType>
	
	<!-- getNotifications päringu tüübid -->
	<complexType name="getNotificationsRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "getNotifications"</documentation>
	      	<documentation xml:lang="en">"getNotifications" request</documentation>
    	</annotation>
		<sequence/>
	</complexType>
	<complexType name="getNotificationsResponse">
		<annotation>
	      	<documentation xml:lang="ee">Päringu "getNotifications" vastus</documentation>
	      	<documentation xml:lang="en">"getNotifications" request response</documentation>
    	</annotation>
		<sequence>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage"/>
			<element name="notifications" type="tns:arrayOfNotification" nillable="false"/>
			<element name="notifications_active" type="boolean">
				<annotation>
			      	<documentation xml:lang="ee">Näitab, kas kasutajal on teavitused aktiivsed</documentation>
			      	<documentation xml:lang="en">Indicates if notifications are active for this user</documentation>
		    	</annotation>
			</element>
			<element name="address_list" type="tns:arrayOfEmailAddress"/>
		</sequence>
	</complexType>
	
	<!-- setNotifications päringu tüübid -->
	<complexType name="setNotificationsRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "setNotifications"</documentation>
	      	<documentation xml:lang="en">"setNotifications" request</documentation>
    	</annotation>
		<sequence>
			<element name="notifications" type="tns:arrayOfNotification" nillable="false"></element>
		</sequence>
	</complexType>
	<complexType name="setNotificationsResponse">
		<annotation>
	      	<documentation xml:lang="ee">Päringu "setNotifications" vastus</documentation>
	      	<documentation xml:lang="en">"setNotifications" request response</documentation>
    	</annotation>
		<sequence>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage"></element>
		</sequence>
	</complexType>
	
	<complexType name="getDocumentListRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "getDocumentList"</documentation>
	      	<documentation xml:lang="en">"getDocumentList" request</documentation>
    	</annotation>
		<sequence>
			<element name="folder" type="string">
				<annotation>
			      	<documentation xml:lang="ee">Dokumentide kaust</documentation>
			      	<documentation xml:lang="en">Folder of documents</documentation>
		    	</annotation>
			</element>
			<element name="document_types" type="tns:arrayOfDocumentType"></element>
			<element name="document_dvk_statuses" type="tns:arrayOfDocumentDvkStatus"></element>
			<element name="document_workflow_statuses" type="tns:arrayOfDocumentWorkflowStatus"></element>
			<element name="has_been_viewed" type="boolean">
				<annotation>
			      	<documentation xml:lang="ee">Näitab, kas antud dokumenti on vaadatud</documentation>
			      	<documentation xml:lang="en">Indicates if the document has been viewed</documentation>
		    	</annotation>
			</element>
			<element name="is_deflated" type="boolean">
				<annotation>
			      	<documentation xml:lang="ee">Näitab, kas antud dokument on arhiveeritud</documentation>
			      	<documentation xml:lang="en">Indicates if the document has been deflated</documentation>
		    	</annotation>
			</element>
			<element name="creator_applications" type="tns:arrayOfCreatorApplication"></element>
			<element name="search_phrase" type="string">
				<annotation>
			      	<documentation xml:lang="ee">Otsingufraas</documentation>
			      	<documentation xml:lang="en">Search phrase</documentation>
		    	</annotation>
			</element>
			<element name="max_results" type="integer">
				<annotation>
			      	<documentation xml:lang="ee">Maksimaalne tulemuste arv</documentation>
			      	<documentation xml:lang="en">Maximum number of returned results</documentation>
		    	</annotation>
			</element>
			<element name="start_index" type="integer">
				<annotation>
			      	<documentation xml:lang="ee">Tagastatavate tulemuste algusindeks</documentation>
			      	<documentation xml:lang="en">Offset(starting index) of the results returned</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="getDocumentListResponse">
		<annotation>
	      	<documentation xml:lang="ee">Päringu "getDocumentList" vastus</documentation>
	      	<documentation xml:lang="en">"getDocumentList" request response</documentation>
    	</annotation>
		<sequence>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage"></element>
			<element name="documentList">
				<annotation>
			      	<documentation xml:lang="ee">Dokumentide massiiv</documentation>
			      	<documentation xml:lang="en">Array of documents</documentation>
		    	</annotation>
				<complexType>
					<attribute name="href" type="tns:href" />
				</complexType>
			</element>
		</sequence>
	</complexType>
	
	<!-- getDocument päringu tüübid -->
	<complexType name="getDocumentRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "getDocument"</documentation>
	      	<documentation xml:lang="en">"getDocument" request</documentation>
    	</annotation>
		<sequence>
			<element name="document_id" type="integer">
				<annotation>
			      	<documentation xml:lang="ee">Dokumendi identifikaator</documentation>
			      	<documentation xml:lang="en">Document identificator</documentation>
		    	</annotation>
			</element>
			<element name="include_file_contents" type="boolean">
				<annotation>
			      	<documentation xml:lang="ee">Näitab, kas tagastatakse ka failide sisu</documentation>
			      	<documentation xml:lang="en">Indicates if the content of the files is returned</documentation>
		    	</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="getDocumentResponse">
		<annotation>
	      	<documentation xml:lang="ee">Päringu "getDocument" vastus</documentation>
	      	<documentation xml:lang="en">"getDocument" request response</documentation>
    	</annotation>
		<sequence>
			<element name="success" type="tns:success" />
			<element name="messages" type="tns:arrayOfMessage"></element>
			<element name="document">
				<annotation>
			      	<documentation xml:lang="ee">Dokumendi andmed</documentation>
			      	<documentation xml:lang="en">Document data</documentation>
		    	</annotation>
				<complexType>
					<attribute name="href" type="tns:href" />
				</complexType>
			</element>
		</sequence>
	</complexType>
	
	<!-- getDocumentFile päringu tüübid -->
	<complexType name="getDocumentFileRequest">
		<annotation>
	      	<documentation xml:lang="ee">Päring "getDocumentFile"</documentation>
	      	<documentation xml:lang="en">"getDocumentFile" request</documentation>
    	</annotation>
		<sequence>
			<element name="document_id" type="integer">
				<annotation>
			      	<documentation xml:lang="ee">Dokumendi identifikaator</documentation>
			      	<documentation xml:lang="en">Document identificator</documentation>
		    	</annotation>
			</element>
			<element name="file_id_list" type="tns:arrayOfFileId"></element>
		</sequence>
	</complexType>
	<complexType name="getDocumentFileResponse">
