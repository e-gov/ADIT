<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions targetNamespace="http://producers.@PRODUCER_NAME@.xtee.riik.ee/producer/@PRODUCER_NAME@"
		xmlns:tns="http://producers.@PRODUCER_NAME@.xtee.riik.ee/producer/@PRODUCER_NAME@"
		
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
		xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
		xmlns:ref="http://ws-i.org/profiles/basic/1.1/xsd"
		xmlns:xrd="http://x-road.eu/xsd/xroad.xsd">

	<wsdl:types>
	
		<schema targetNamespace="http://producers.@PRODUCER_NAME@.xtee.riik.ee/producer/@PRODUCER_NAME@" xmlns="http://www.w3.org/2001/XMLSchema">
				
			<import namespace="http://ws-i.org/profiles/basic/1.1/xsd" schemaLocation="http://ws-i.org/profiles/basic/1.1/swaref.xsd" />
			<import namespace="http://x-road.eu/xsd/xroad.xsd" schemaLocation="http://x-road.eu/xsd/xroad.xsd" />
			
			<xs:annotation>
				<xs:documentation xml:lang="ee">
					ADIT (Avalike Dokumentide Infrastruktuuri Teenus) päringutes kasutatavate andmete kirjeldus "Versioon 2".
					Dokument sisaldab nii (SOAP) päringute keha elementide kui ka päringute manustes kasutatavate andmete kirjeldusi.
					Litsenseeritud [LITSENTS] alusel.
				</xs:documentation>
				<xs:documentation xml:lang="en">
					Describes the "Version 2" data types used in ADIT(Infrastructure Service for Public Documents) requests.
					The Documents contains both the data types used in (SOAP) request body elements and attachments.
					Licensed under [LICENSE].
				</xs:documentation>
			</xs:annotation>

			<element name="getDocumentRequest" type="tns:getDocumentV2Request" />
			<element name="getDocumentResponse" type="tns:getDocumentV2Response" />
			<element name="getUserInfoV2Request" type="tns:getUserInfoV2Request" />
			<element name="getUserInfoV2Response" type="tns:getUserInfoV2Response" />
			<element name="sendDocumentV2Request" type="tns:sendDocumentV2Request" />
			<element name="sendDocumentV2Response" type="tns:sendDocumentV2Response" />
			<element name="getDocumentListV2Request" type="tns:getDocumentListV2Request" />
			<element name="getDocumentListV2Response" type="tns:getDocumentListV2Response" />
			<element name="getSendStatusV2Request" type="tns:getSendStatusV2Request" />
			<element name="getSendStatusV2Response" type="tns:getSendStatusV2Response" />

			<element name="infosysteem" type="string">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Infosüsteemi lühinimi</xs:documentation>
					<xs:documentation xml:lang="en">Remote application short name</xs:documentation>
				</xs:annotation>
			</element>

			<simpleType name="success">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Näitab, kas päring õnnestus</xs:documentation>
					<xs:documentation xml:lang="en">Indicates if the request was successful</xs:documentation>
				</xs:annotation>
				<restriction base="boolean" />
			</simpleType>

			<simpleType name="href">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Viide manusele</xs:documentation>
					<xs:documentation xml:lang="en">Attachment reference</xs:documentation>
				</xs:annotation>
				<restriction base="ref:swaRef" />
			</simpleType>

			<complexType name="message">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Teade</xs:documentation>
					<xs:documentation xml:lang="en">Message</xs:documentation>
				</xs:annotation>
				<simpleContent>
					<extension base="string">
						<attribute name="lang" type="string" />
					</extension>
				</simpleContent>
			</complexType>

			<complexType name="arrayOfMessage">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Teadete massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of messages</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="message" type="tns:message" />
				</sequence>
			</complexType>

			<complexType name="arrayOfFileType">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Faili tüüpide massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of file types</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="file_type">
						<xs:annotation>
							<xs:documentation xml:lang="ee">Faili tüüp</xs:documentation>
							<xs:documentation xml:lang="en">File type</xs:documentation>
						</xs:annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="document_file" />
								<enumeration value="signature_container" />
								<enumeration value="zip_archive" />
							</restriction>
						</simpleType>
					</element>
				</sequence>
			</complexType>
			<complexType name="documentActionStatus">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Dokumendi toimingu staatus</xs:documentation>
					<xs:documentation xml:lang="en">Document action status</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="document_id" type="integer">
						<xs:annotation>
							<xs:documentation xml:lang="ee">Dokumendi identifikaator</xs:documentation>
							<xs:documentation xml:lang="en">Document identificator</xs:documentation>
						</xs:annotation>
					</element>
					<element name="success" type="tns:success" />
					<element name="messages" type="tns:arrayOfMessage" />
				</sequence>
			</complexType>
			<complexType name="arrayOfDocumentWorkflowStatus">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Dokumendi töövoo staatuste identifikaatorite massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of document workflow status identificators</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="status_id" type="integer">
						<xs:annotation>
							<xs:documentation xml:lang="ee">Dokumendi töövoo staatuse identifikaator</xs:documentation>
							<xs:documentation xml:lang="en">Document workflow status identificator</xs:documentation>
						</xs:annotation>
					</element>
				</sequence>
			</complexType>
			<complexType name="arrayOfDocumentDvkStatus">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Dokumendi DVK(Dokumendivahetuskeskus) staatuste identifikaatorite massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of document DVK(Dokumendivahetuskeskus) status identificators</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="status_id" type="integer">
						<xs:annotation>
							<xs:documentation xml:lang="ee">Dokumendi DVK(Dokumendivahetuskeskus) staatuse identifikaator</xs:documentation>
							<xs:documentation xml:lang="en">Document DVK(Dokumendivahetuskeskus) status identificator</xs:documentation>
						</xs:annotation>
					</element>
				</sequence>
			</complexType>
			<complexType name="arrayOfDocumentType">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Dokumendi tüüpide massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of document types</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="document_type" type="string">
						<xs:annotation>
							<xs:documentation xml:lang="ee">Dokumendi tüüp</xs:documentation>
							<xs:documentation xml:lang="en">Document type</xs:documentation>
						</xs:annotation>
					</element>
				</sequence>
			</complexType>
			<complexType name="arrayOfRecipientStatus">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Vastuvõtja(adressaadi) staatuste massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of recipient statuses</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="recipient" type="tns:recipientStatus" />
				</sequence>
			</complexType>
			<complexType name="recipientStatus">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Vastuvõtja(adressaat) staatus</xs:documentation>
					<xs:documentation xml:lang="en">Recipient status</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="code" type="string">
						<xs:annotation>
							<xs:documentation xml:lang="ee">Vastuvõtja(adressaadi) kood</xs:documentation>
							<xs:documentation xml:lang="en">Recipient code</xs:documentation>
						</xs:annotation>
					</element>
					<element name="success" type="tns:success" />
					<element name="messages" type="tns:arrayOfMessage" />
				</sequence>
			</complexType>
			<complexType name="arrayOfDocumentActionStatus">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Dokumendi toimingu staatuste massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of document action statuses</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="document" type="tns:documentActionStatus" />
				</sequence>
			</complexType>

			<complexType name="arrayOfCreatorApplication">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Dokumendi loomiseks kasutatud infosüsteemide lühinimede massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of the names of the applications used to create documents</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="creator_application" type="string">
						<xs:annotation>
							<xs:documentation xml:lang="ee">Dokumendi loomiseks kasutatud infosüsteemi lühinimi</xs:documentation>
							<xs:documentation xml:lang="en">Name of the application used to create documents</xs:documentation>
						</xs:annotation>
					</element>
				</sequence>
			</complexType>

			<complexType name="arrayOfUserCode">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Kasutajakoodide massiiv</xs:documentation>
					<xs:documentation xml:lang="en">Array of user codes</xs:documentation>
				</xs:annotation>
				<sequence>
					<element maxOccurs="unbounded" minOccurs="0" name="code" type="string">
						<xs:annotation>
							<xs:documentation xml:lang="ee">Kasutajakood</xs:documentation>
							<xs:documentation xml:lang="en">User codes</xs:documentation>
						</xs:annotation>
					</element>
				</sequence>
			</complexType>

			<!-- getDocument päringu tüübid -->
			<complexType name="getDocumentV2Request">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päring "getDocument"</xs:documentation>
					<xs:documentation xml:lang="en">"getDocument" request</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="document_id" type="integer">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Dokumendi identifikaator</xs:documentation>
										<xs:documentation xml:lang="en">Document identificator</xs:documentation>
									</xs:annotation>
								</element>
								<element name="include_file_contents" type="boolean">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Näitab, kas tagastatakse ka failide sisu</xs:documentation>
										<xs:documentation xml:lang="en">Indicates if the content of the files is returned</xs:documentation>
									</xs:annotation>
								</element>
								<element name="file_types" type="tns:arrayOfFileType" />
								<element name="document_guid" type="string">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Dokumendi globaalselt unikaalne identifikaator</xs:documentation>
										<xs:documentation xml:lang="en">Document generally unique identificator</xs:documentation>
									</xs:annotation>
								</element>
								<element name="dvk_id" type="integer">
									<xs:annotation>
										<xs:documentation xml:lang="ee">DVK id</xs:documentation>
										<xs:documentation xml:lang="en">DVK id</xs:documentation>
									</xs:annotation>
								</element>
							</sequence>
						</complexType>
					</element>
				</sequence>
			</complexType>
			<complexType name="getDocumentV2Response">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päringu "getDocument" vastus</xs:documentation>
					<xs:documentation xml:lang="en">"getDocument" request response</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="success" type="tns:success" />
								<element name="messages" type="tns:arrayOfMessage" />
								<element name="document">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Dokumendi andmed</xs:documentation>
										<xs:documentation xml:lang="en">Document data</xs:documentation>
									</xs:annotation>
									<complexType>
										<attribute name="href" type="tns:href" />
									</complexType>
								</element>
							</sequence>
						</complexType>
					</element>
				</sequence>
			</complexType>
			
			<!-- getUserInfo päringu tüübid -->
			<complexType name="getUserInfoV2Request">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päring "getUserInfo"</xs:documentation>
					<xs:documentation xml:lang="en">"getUserInfo" request</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="userList">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Kasutajate massiiv</xs:documentation>
										<xs:documentation xml:lang="en">Array of users</xs:documentation>
									</xs:annotation>
									<complexType>
										<attribute name="href" type="tns:href" />
									</complexType>
								</element>
							</sequence>
						</complexType>
					</element>
				</sequence>
			</complexType>
			<complexType name="getUserInfoV2Response">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päringu "getUserInfo" vastus</xs:documentation>
					<xs:documentation xml:lang="en">"getUserInfo" request response</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="success" type="tns:success" />
								<element name="messages" type="tns:arrayOfMessage" />
								<element name="userList">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Kasutajate andmete massiiv</xs:documentation>
										<xs:documentation xml:lang="en">Array of user data</xs:documentation>
									</xs:annotation>
									<complexType>
										<attribute name="href" type="tns:href" />
									</complexType>
								</element>
							</sequence>
						</complexType>
					</element>
				</sequence>
			</complexType>
			
			<!-- sendDocument päringu tüübid -->
			<complexType name="sendDocumentV2Request">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päring "sendDocument"</xs:documentation>
					<xs:documentation xml:lang="en">"sendDocument" request</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="document_id" nillable="false" type="integer">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Dokumendi identifikaator</xs:documentation>
										<xs:documentation xml:lang="en">Document identificator</xs:documentation>
									</xs:annotation>
								</element>
								<element name="recipient_list" type="tns:arrayOfUserCode" />
								<element name="dec_folder" type="string">
									<xs:annotation>
										<xs:documentation xml:lang="ee">DVK dokumentide kaust</xs:documentation>
										<xs:documentation xml:lang="en">DVK Folder of documents</xs:documentation>
									</xs:annotation>
								</element>
							</sequence>
						</complexType>
					</element>
				</sequence>
			</complexType>
			<complexType name="sendDocumentV2Response">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päringu "sendDocument" vastus</xs:documentation>
					<xs:documentation xml:lang="en">"sendDocument" request response</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="success" type="tns:success" />
								<element name="messages" type="tns:arrayOfMessage" />
								<element name="recipient_list" type="tns:arrayOfRecipientStatus" />
							</sequence>
						</complexType>
					</element>
				</sequence>
			</complexType>
						<complexType name="getDocumentListV2Request">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päring "getDocumentList"</xs:documentation>
					<xs:documentation xml:lang="en">"getDocumentList" request</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="folder" type="string">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Dokumentide kaust</xs:documentation>
										<xs:documentation xml:lang="en">Folder of documents</xs:documentation>
									</xs:annotation>
								</element>
								<element name="dec_folder" type="string">
									<xs:annotation>
										<xs:documentation xml:lang="ee">DVK dokumentide kaust</xs:documentation>
										<xs:documentation xml:lang="en">DVK Folder of documents</xs:documentation>
									</xs:annotation>
								</element>
								<element name="document_types" type="tns:arrayOfDocumentType" />
								<element name="file_types" type="tns:arrayOfFileType" />
								<element name="document_dhx_statuses" type="tns:arrayOfDocumentDvkStatus" />
								<element name="document_workflow_statuses" type="tns:arrayOfDocumentWorkflowStatus" />
								<element name="has_been_viewed" type="boolean">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Näitab, kas antud dokumenti on vaadatud</xs:documentation>
										<xs:documentation xml:lang="en">Indicates if the document has been viewed</xs:documentation>
									</xs:annotation>
								</element>
								<element name="signed" type="boolean">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Näitab, kas antud dokument on allkirjastatud</xs:documentation>
										<xs:documentation xml:lang="en">Indicates if the document has been signed</xs:documentation>
									</xs:annotation>
								</element>
								<element name="is_deflated" type="boolean">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Näitab, kas antud dokument on arhiveeritud</xs:documentation>
										<xs:documentation xml:lang="en">Indicates if the document has been deflated</xs:documentation>
									</xs:annotation>
								</element>
								<element name="creator_applications" type="tns:arrayOfCreatorApplication" />
								<element name="search_phrase" type="string">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Otsingufraas</xs:documentation>
										<xs:documentation xml:lang="en">Search phrase</xs:documentation>
									</xs:annotation>
								</element>
								<element name="period_start" type="date">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Viimase muudatuse vahemiku algus</xs:documentation>
										<xs:documentation xml:lang="en">Start date of last modified range</xs:documentation>
									</xs:annotation>
								</element>
								<element name="period_end" type="date">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Viimase muudatuse vahemiku lõpp</xs:documentation>
										<xs:documentation xml:lang="en">End date of last modified range</xs:documentation>
									</xs:annotation>
								</element>
								<element name="max_results" type="integer">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Maksimaalne tulemuste arv</xs:documentation>
										<xs:documentation xml:lang="en">Maximum number of returned results</xs:documentation>
									</xs:annotation>
								</element>
								<element name="start_index" type="integer">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Tagastatavate tulemustealgusindeks</xs:documentation>
										<xs:documentation xml:lang="en">Offset(starting index) of the results returned</xs:documentation>
									</xs:annotation>
								</element>
								<element name="sort_by">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Andmeväli, mille järgi otsingu tulemused sorteeritakse</xs:documentation>
										<xs:documentation xml:lang="en">Name of field that will be used to sort search result</xs:documentation>
									</xs:annotation>
									<simpleType>
										<restriction base="string">
											<enumeration value="created" />
											<enumeration value="document_type" />
											<enumeration value="dvk_id" />
											<enumeration value="guid" />
											<enumeration value="id" />
											<enumeration value="last_modified" />
											<enumeration value="title" />
											<enumeration value="file_size" />
											<enumeration value="sender_receiver" />
										</restriction>
									</simpleType>
								</element>
								<element name="sort_order">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Otsingutulemuste sorteerimise suund</xs:documentation>
										<xs:documentation xml:lang="en">Search result sort order</xs:documentation>
									</xs:annotation>
									<simpleType>
										<restriction base="string">
											<enumeration value="asc" />
											<enumeration value="desc" />
										</restriction>
									</simpleType>
								</element>
							</sequence>
						</complexType>
					</element>
				</sequence>
			</complexType>
			<complexType name="getDocumentListV2Response">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päringu "getDocumentList" vastus</xs:documentation>
					<xs:documentation xml:lang="en">"getDocumentList" request response</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="success" type="tns:success" />
								<element name="messages" type="tns:arrayOfMessage" />
								<element name="documentList">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Dokumentide massiiv</xs:documentation>
										<xs:documentation xml:lang="en">Array of documents</xs:documentation>
									</xs:annotation>
									<complexType>
										<attribute name="href" type="tns:href" />
									</complexType>
								</element>
							</sequence>
						</complexType>
					</element>
				</sequence>
			</complexType>
			
			<!-- getSendStatus päringu tüübid -->
			<complexType name="getSendStatusV2Request">
				<xs:annotation>
					<xs:documentation xml:lang="ee">Päring "getSendStatus"</xs:documentation>
					<xs:documentation xml:lang="en">"getSendStatus" request</xs:documentation>
				</xs:annotation>
				<sequence>
					<element name="keha">
						<complexType>
							<sequence>
								<element name="documendid">
									<xs:annotation>
										<xs:documentation xml:lang="ee">Dokumendid</xs:documentation>
										<xs:documentation xml:lang="en">Documents</xs:documentation>
									</xs:annotation>
									<complexType>
										<attribute name="href" type="tns:href" />
									</complexType>
								</element>
							</sequence>
						</complexType>
					</element>
